import heapq
nums = [1, 8, 2, 23, 7, -4, 18, 23, 42, 37, 2]
heapq.heapify(nums)
heapq.heappop(nums)
heapq.nlargest(3, nums)
heapq.nsmallest(3, nums)
heapq.heappush(reversed_nums, -num)

